{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "composite": true,
    "declarationMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./src/*"],
      "@sadellari-dao/sdk/*": ["../../../packages/sdk/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "references": [
    {
      "path": "../../../packages/sdk"
    }
  ]
}
